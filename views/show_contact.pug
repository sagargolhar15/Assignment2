include ./header.pug 

.container
  h2.margin.text-center.text-primary Contact Details

  table.table.table-bordered
    thead.table-success
      tr
        th Name
        th Email
        th City
        th Subject
    tbody
      - for (var i = 0; i < result.length-1; ++i) {
        tr
        td #{result[i].name}
        td #{result[i].email}
        td #{result[i].city}  
        td #{result[i].subject}
      - }
  